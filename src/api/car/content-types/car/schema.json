{
  "kind": "collectionType",
  "collectionName": "cars",
  "info": {
    "singularName": "car",
    "pluralName": "cars",
    "displayName": "car",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "model": {
      "type": "string"
    },
    "color": {
      "type": "string"
    },
    "price": {
      "type": "decimal"
    },
    "seats": {
      "type": "integer"
    },
    "featured": {
      "type": "boolean"
    },
    "newArrivals": {
      "type": "boolean"
    },
    "condition": {
      "type": "enumeration",
      "enum": [
        "Used",
        "New"
      ]
    },
    "fuelType": {
