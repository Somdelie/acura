      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "transimition": {
      "type": "enumeration",
      "enum": [
        "Auto",
        "Manual",
        "Gsg"
      ]
    },
    "doors": {
      "type": "integer"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "cars"
    },
    "make": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::make.make",
      "inversedBy": "cars"
